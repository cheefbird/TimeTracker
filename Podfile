# Uncomment the next line to define a global platform for your project
 platform :ios, '10.0'

target 'TimeTracker' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TimeTracker
  pod 'RxSwift', :inhibit_warnings => true
  pod 'RxCocoa', :inhibit_warnings => true
  pod 'SwiftyJSON', :inhibit_warnings => true
  pod 'RealmSwift', :inhibit_warnings => true
  pod 'Alamofire', :inhibit_warnings => true
  pod 'RxAlamofire', :inhibit_warnings => true

  target 'TimeTrackerTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'TimeTrackerUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
